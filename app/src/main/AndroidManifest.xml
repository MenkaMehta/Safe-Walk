<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mad.safewalk">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_login"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_login"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".EmergencyContactsActivity"
            android:label="@string/title_activity_emergency_contacts"></activity>
        <activity android:name=".ResetPasswordActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".AddContact"></activity>
        <activity android:name=".MessageActivity"></activity>
    </application>

</manifest>